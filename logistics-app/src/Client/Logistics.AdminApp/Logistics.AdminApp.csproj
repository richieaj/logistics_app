        <PackageReference Include="Radzen.Blazor" Version="5.9.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Logistics.HttpClient\Logistics.HttpClient.csproj" />
    </ItemGroup>
</Project>
